{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/gmon.out": true,
        "**/__pycache__": true,
        "**/.venv": true,
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/.mutmut-cache": true,
        "**/junit.xml": true,
        "**/coverage.xml": true,
        "**/.coverage": true,
        "**/coverage-report": true,
        "**/*.egg-info": true,
        "dist/": true,
    },
    "python.formatting.provider": "black",
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": true,
}